@media(prefers-color-scheme:dark){:root{--background:$base03;--background-secondary:$base02;--text:$base0;--text-secondary:$base01;--text-emphasized:$base1}}@media(prefers-color-scheme:light){:root{--background:$base3;--background-secondary:$base2;--text:$base00;--text-secondary:$base1;--text-emphasized:$base01}}:root{--yellow:$yellow;--orange:$orange;--red:$red;--magenta:$magenta;--violet:$violet;--blue:$blue;--cyan:$cyan;--green:$green}body{background-color:var(--background);color:var(--text)}hr{background-color:var(--text-secondary)}#wrapper{background-color:var(--background)}#main .toc{background-color:var(--background-secondary)}blockquote{background:var(--background-secondary)}.content pre{background-color:var(--background-secondary)!important;color:var(--text)}code{background-color:var(--background)!important;color:var(--text-emphasized)!important}button.copyButton{background-color:var(--background);color:var(--text-secondary)}a:link{color:var(--blue)}a:visited{color:var(--violet)}a:hover{color:var(--violet)}nav a{color:var(--text-emphasized)!important}nav a:after{background-color:var(--text)}h2 a{color:var(--text-emphasized)!important}h2.header a:hover,h2.header a:visited,h2.header a.current{color:var(--text-emphasized)}time{color:var(--text-secondary)}table{margin:auto;background-color:var(--background);border-top:1px solid var(--text);border-bottom:1px solid var(--text)}table thead th{border-bottom:1px solid var(--text)}table th{background:var(--background-secondary)!important;color:var(--text-emphasized)!important}table td{padding:5px}.nav-list{border-color:var(--text-secondary)}.sidebar ul li a{color:var(--text)}.sidebar ul li a:hover{color:var(--text-emphasized);background-color:var(--background-secondary)}